<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	
	 <bean class="com.springcore.autowiring.Address" name="address">
		<property name="street" value="3"/>
		<property name="city" value="hongkong"/>
		</bean>
		
	<bean class="com.springcore.autowiring.Employee" name="emp1" autowire="constructor">
		<property name="id" value="1"/>
		<property name="name" value="Hong yun"/>
		<!--<property name="Address" ref="add1"/>-->
		</bean>	
</beans>