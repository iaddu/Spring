<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	
	<bean class="com.springcore.collections.Student" name="std1">
		<property name="studentPhones">
			<list>
				<value>2332232323</value>
				<value>8778987677</value>
			</list>
		</property>
		<property name="studentAddresses">
			<set>
				<value>Berlin</value>
				<value>Tokyo</value>
			</set>
		</property>
		<property name="studentCourses">
			<map>
				<entry key="c1" value="Maths"></entry>
				<entry key="c2" value="Science"></entry>
			</map>
		</property>
		
	</bean> 
	
</beans>